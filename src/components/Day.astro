---
import { format } from "date-fns";
import Match from "./Match.astro";
import { MatchProps } from "./Match.astro";

export interface DayProps {
    day: Date
    matches: MatchProps[]
}
const { day, matches } = Astro.props as DayProps;
---
<p>{format(day, "PPP")}</p>
{matches.map(match => <Match {...match}></Match>)}
